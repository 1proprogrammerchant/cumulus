{
  "name": "gibs-dashboard",
  "version": "0.1.0",
  "description": "A dashboard for controlling GIBS in AWS.",
  "repository": {
    "type": "git",
    "url": "https://git.earthdata.nasa.gov/projects/GITC/repos/gibs-dashboard"
  },
  "author": {
    "name": "NASA EOSDIS",
    "url": "https://earthdata.nasa.gov/"
  },
  "license": "TBD",
  "homepage": "https://git.earthdata.nasa.gov/projects/GITC/repos/gibs-dashboard/browse",
  "scripts": {
    "//": "Creates a local.js file after install if it hasn't been created.",
    "//": "TODO is this used for anything?",
    "postinstall": "[ -f app/scripts/config/local.js ] || echo 'module.exports = {};' > app/scripts/config/local.js",

    "//": "Starts serving the files locally",
    "serve": "GIBS_ENV=development gulp serve",

    "//": "Builds for staging",
    "//": "TODO do we want this for GIBS dashboard? Remove in other places if necessary.",
    "staging": "GIBS_ENV=staging gulp",

    "//": "Builds for production",
    "production": "GIBS_ENV=production gulp",


    "lint": "eslint app/scripts/ --ext .js",
    "start": "npm run serve",

    "//": "TODO add additional testing",
    "test": "npm run lint"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },


