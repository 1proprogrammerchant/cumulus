{
  "name": "gitc",
  "version": "0.0.0",
  "description": "GIBS-in-the-Cloud Prototype",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && mocha-webpack",
    "ci": "./node_modules/.bin/eslint -o ../test-reports/eslint.xml . -f junit && ./node_modules/.bin/mocha-webpack --reporter mocha-junit-reporter --reporter-options mochaFile=../test-reports/testreport.xml",
    "start": "webpack -w --colors",
    "prepublish": "find . -name package.json -not -path './test/*' -not -path '*/node_modules/*' -not -path './package.json' -execdir npm install \\; && webpack",
    "prune": "npm prune && find . -name package.json -not -path './test/*' -not -path '*/node_modules/*' -not -path './package.json' -execdir npm prune \\;",
    "lock": "npm run prune && npm shrinkwrap --dev && find . -name package.json -not -path './test/*' -not -path '*/node_modules/*' -not -path './package.json' -execdir npm shrinkwrap --dev \\; && npm install"
  },
  "keywords": [
    "GIBS"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@git.earthdata.nasa.gov:7999/gitc/gibs-prototype.git"
  },
  "author": "Patrick Quinn",
  "license": "Apache-2.0",
  "dependencies": {
    "copy-webpack-plugin": "^3.0.1",
    "glob": "^7.0.5"
  },
  "devDependencies": {
    "aws-sdk": "^2.4.9",
    "aws-sdk-mock": "^1.3.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "expect.js": "^0.3.1",
    "exports-loader": "^0.6.3",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "mocha-webpack": "^0.7.0",
    "prepend-loader": "0.0.2",
    "sinon": "^1.17.4",
    "source-map-support": "^0.4.2",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  }
}
