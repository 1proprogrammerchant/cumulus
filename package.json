{
  "name": "gitc",
  "version": "0.0.0",
  "description": "GIBS-in-the-Cloud Prototype",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && mocha-webpack",
    "ci": "./bin/npm/ci",
    "start": "webpack -w --colors",
    "prepublish": "./bin/npm/prepublish",
    "lock": "./bin/npm/lock",
    "clean": "./bin/npm/clean",
    "//": [
      "Allows validation of a collection configuration file",
      "Execute like `npm run-script validate-collection-json -- docs/schemas/example-data/example-collection.json`"
    ],
    "generate-docs": "./node_modules/.bin/generate-md --layout github --input docs/ingest_workflows_architecture.md --output docs",
    "serve-docs": "./node_modules/.bin/http-server -c-1 docs -o",
    "validate-collection-json": "sed 's/!GitcResource//g' config/collections.yml | js-yaml > tmp_collection.json && ajv validate -s docs/schemas/collections_config_schema.json -r docs/schemas/ingest_common_schema.json -d tmp_collection.json; rm tmp_collection.json",
    "validate-example-envelope": "ajv validate -s docs/schemas/envelope_schema.json -r docs/schemas/ingest_common_schema.json -d docs/schemas/example-data/example-message-envelope.json"
  },
  "keywords": [
    "GIBS"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@git.earthdata.nasa.gov:7999/gitc/gibs-prototype.git"
  },
  "author": "Patrick Quinn",
  "license": "Apache-2.0",
  "dependencies": {
    "copy-webpack-plugin": "^3.0.1",
    "glob": "^7.0.5"
  },
  "devDependencies": {
    "ajv": "^5.0.4-beta.3",
    "ajv-cli": "^2.0.0",
    "aws-sdk": "^2.4.9",
    "aws-sdk-mock": "^1.3.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "expect.js": "^0.3.1",
    "exports-loader": "^0.6.3",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "markdown-styles": "^3.1.9",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "mocha-webpack": "^0.7.0",
    "prepend-loader": "0.0.2",
    "sinon": "^2.2.0",
    "source-map-support": "^0.4.2",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  }
}
